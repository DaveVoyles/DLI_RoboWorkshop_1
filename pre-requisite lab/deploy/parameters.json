{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "eastus"
        },
        "virtualMachines_vm_name": {
            "value": "dlirw<name>-vm"
        },
        "networkInterfaces_nic_name": {
            "value": "dlirw<name>-nic"
        },
        "networkSecurityGroups_nsg_name": {
            "value": "dlirw<name>-nsg"
        },
        "publicIPAddresses_ip_name": {
            "value": "dlirw<name>-ip"
        },
        "virtualNetworks_vnet_name": {
            "value": "dlirw<name>-vnet"
        },
        "storageAccounts_account_name": {
            "value": "dlirw<name>-storage"
        }
    }
}